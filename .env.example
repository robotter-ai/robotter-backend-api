# Environment
ENV=development  # Options: development, production, test

# API Keys (required)
BIRDEYE_API_KEY=your_birdeye_api_key_here
FASTAPI_WALLETAUTH_PRIVATE_KEY=your_wallet_auth_private_key_here

# Security
GATEWAY_CERT_PASSPHRASE=strong_passphrase_here
CONFIG_PASSWORD=strong_password_here

# Paths (relative to project root recommended)
BOTS_PATH=./bots
CERTS_PATH=./certs

# API Configuration
CORS_ORIGINS=["http://localhost:3000", "https://your-frontend-domain.com"]  # JSON array or "*" for all
API_V1_PREFIX=/api/v1 